{
  "name": "@betaflight/msp",
  "version": "0.1.1",
  "description": "A MultiWii Serial Protocol library to connect and communicate with flight controllers",
  "license": "MIT",
  "author": "freshollie@gmail.com",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/cjs/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/freshollie/fresh-configurator/tree/master/packages/msp",
  "readme": "https://github.com/freshollie/fresh-configurator/tree/master/packages/msp#readme",
  "keywords": [
    "betaflight",
    "flight controller",
    "msp",
    "multiwii",
    "parser",
    "multiwii protocol"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "testscript": "ts-node -T test-script.ts",
    "build": "rimraf dist && concurrently 'yarn build:esm' 'yarn build:cjs'",
    "build:esm": "tsc -p tsconfig.build.json --module es2015 --outDir dist/esm",
    "build:cjs": "tsc -p tsconfig.build.json --outDir dist/cjs"
  },
  "dependencies": {
    "@serialport/bindings": "^8.0.7",
    "@serialport/stream": "^8.0.6",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@serialport/binding-mock": "^8.0.6",
    "@types/debug": "^4.1.5",
    "@types/serialport": "^8.0.0",
    "concurrently": "^5.2.0",
    "flush-promises": "^1.0.2",
    "rimraf": "^3.0.2",
    "typescript": ">=3.7"
  }
}
