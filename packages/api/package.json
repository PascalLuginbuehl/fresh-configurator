{
  "name": "@betaflight/api",
  "version": "0.1.1",
  "description": "A high-level API to read data from betaflight flight controllers",
  "author": "freshollie@gmail.com",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/cjs/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/freshollie/fresh-configurator/tree/master/packages/api",
  "readme": "https://github.com/freshollie/fresh-configurator/tree/master/packages/api#readme",
  "license": "MIT",
  "keywords": [
    "betaflight",
    "api",
    "flightcontroller",
    "msp",
    "reader"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && concurrently 'yarn build:esm' 'yarn build:cjs'",
    "build:esm": "tsc -p tsconfig.build.json --module es2015 --outDir dist/esm",
    "build:cjs": "tsc -p tsconfig.build.json --outDir dist/cjs"
  },
  "dependencies": {
    "@betaflight/msp": "^0.1.1",
    "rambda": "^4.5.0",
    "semver": "^7.1.3"
  },
  "devDependencies": {
    "@types/semver": "^7.1.0",
    "concurrently": "^5.2.0",
    "typescript": ">=3.7"
  }
}
