{
  "name": "@fresh/configurator",
  "version": "1.0.0",
  "main": "./app/main.js",
  "license": "MIT",
  "build": {
    "productName": "Fresh Configurator",
    "appId": "com.freshollie.configurator",
    "mac": {
      "category": "your.app.category.type"
    },
    "linux": {
      "category": "Settings",
      "executableName": "fresh-configurator",
      "artifactName": "${productName}-${version}.${ext}"
    },
    "win": {
      "target": "msi"
    }
  },
  "scripts": {
    "start": "webpack --mode=development --config ./webpack.config.js && electron ./app/main.js",
    "build": "webpack --mode=production --config ./webpack.config.js && electron-builder",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.38",
    "@fresh/msp": "^1.0.0",
    "date-fns": "^2.11.0",
    "graphql": "^14.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.12.0",
    "react-markdown": "^4.3.1",
    "react-switchery": "^1.2.3",
    "react-three-fiber": "^4.0.14",
    "semver": "^7.1.3",
    "styled-components": "^5.0.1",
    "three": "0.98.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0-beta.1",
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-docs": "^5.3.12",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addons": "^5.3.12",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.3.12",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/semver": "^7.1.0",
    "@types/styled-components": "^4.4.2",
    "@types/three": "0.93.31",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "electron": "7.1.11",
    "electron-builder": "^22.4.1",
    "file-loader": "^5.1.0",
    "fork-ts-checker-webpack-plugin": "^4.0.3",
    "html-webpack-plugin": "^3.2.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-svg-loader": "^3.0.3",
    "svg-inline-loader": "^0.8.0",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "typescript": ">=3.7"
  }
}
